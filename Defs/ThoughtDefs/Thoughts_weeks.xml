    <stages>
      <li>
        <label>nice work</label>
		    <description>i like to do things</description>
        <baseMoodEffect>7</baseMoodEffect>
      </li>
      <li>
        <label>impactful labor</label>
		    <description>nice day to hard work</description>
        <baseMoodEffect>14</baseMoodEffect>
      </li>
      <li>
        <label>work, again</label>
		    <description>i am tired with all this labor</description>
        <baseMoodEffect>-4</baseMoodEffect>
      </li>
       <li>
        <label>hate this week</label>
		    <description>this whole week should be a holiday, not a work!</description>
